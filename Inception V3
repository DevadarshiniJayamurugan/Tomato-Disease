ot as plt"],"metadata":{"id":"dOjAWPFFjYiU"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# plot the loss\n","plt.plot(r.history['loss'], label='train loss')\n","plt.plot(r.history['val_loss'], label='val loss')\n","plt.legend()\n","plt.show()\n","plt.savefig('LossVal_loss')\n","\n","# plot the accuracy\n","plt.plot(r.history['accuracy'], label='train acc')\n","plt.plot(r.history['val_accuracy'], label='val acc')\n","plt.legend()\n","plt.show()\n","plt.savefig('AccVal_acc')"],"metadata":{"id":"edLkqvOpjD7a"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# save it as a h5 file\n","\n","\n","from tensorflow.keras.models import load_model\n","\n","model.save('model_inception.h5')"],"metadata":{"id":"34dtp0-ujiJ8"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["y_pred = model.predict(test_set)"],"metadata":{"id":"Jw5WpsSojiMs"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["y_pred"],"metadata":{"id":"BsS2fD_bjiPn"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["import numpy as np\n","y_pred = np.argmax(y_pred, axis=1)"],"metadata":{"id":"-wes7qvkjiS6"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["y_pred"],"metadata":{"id":"U2Yy0PCTj2ji"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from tensorflow.keras.models import load_model\n","from tensorflow.keras.preprocessing import image"],"metadata":{"id":"5O8duycpj2lw"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["model=load_model('model_resnet50.h5')"],"metadata":{"id":"8ObJmyUsj7t5"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["img_data"],"metadata":{"id":"fPYiCJPsj7w6"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["img=image.load_img('Datasets/Test/Coffee/download (2).jpg',target_size=(224,224))"],"metadata":{"id":"KtpGxEv9j70R"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["x=image.